{
	"compilerOptions": {
		"target": "ES2022",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"rewriteRelativeImportExtensions": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./lib"
	},
	"include": ["src"],
	"exclude": ["node_modules", "test"]
}
